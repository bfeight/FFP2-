<%= form_for ([@user, @visual]), html: { multipart: true } do |f| %>


  <p>
     <%= f.file_field :picture %>
    <br>
    <!-- Or Provide a URL: <%#= f.text_field :picture_url %><br> -->
    <%= f.submit 'Submit' %>
  </p>

  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.submit %>
  </p>

<% end %>
